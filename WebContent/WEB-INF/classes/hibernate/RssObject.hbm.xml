<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="rss.bean.RssObject" table="RssObject">
	    <id name="id" type="java.lang.Long">
	        <column name="id" />
			<generator class="native" />
	    </id>
	    <property generated="never" name="title" lazy="false" type="java.lang.String" length="255">
	        <column name="title"/>
	    </property>
	    <property generated="never" name="link" lazy="false" type="java.lang.String" length="500">
	        <column name="link"/>
	    </property>
	</class>
</hibernate-mapping>